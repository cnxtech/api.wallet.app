{
  "name": "twinswallet-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-server": "node app.js",
    "build": "ng build",
    "bundle": "ng build --aot  --vendor-chunk=false --output-hashing=none --deploy-url /dist/ && node app.js",
    "build-prod": "ng build --delete-output-path=false --aot --prod  --vendor-chunk=false --output-hashing=none --deploy-url /dist/",
    "build-prod2": "ng build --delete-output-path=false --aot --prod  --vendor-chunk=false --output-hashing=none --deploy-url /dist/",
    "build-prod-start": "ng build --delete-output-path=false--aot --prod  --vendor-chunk=false --output-hashing=none --deploy-url /dist/ && node app.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-coin-list-widget": "ng build --delete-output-path=false --aot --prod --project coin-list-widget --vendor-chunk=false --output-hashing=none && node concat.js coin-list-widget",
    "______bundle-coin-list-widget": "ng build --delete-output-path=false --aot --prod --project coin-list-widget --vendor-chunk=false --output-hashing=none && node concat.js src/assets/widgets/coin-list-widget/",
    "bundle-coin-price-marquee-widget": "ng build --delete-output-path=false --aot --prod --project coin-price-marquee-widget --vendor-chunk=false --output-hashing=none && node concat.js coin-price-marquee-widget",
    "______bundle-coin-price-marquee-widget": "ng build --delete-output-path=false --aot --prod --project coin-price-marquee-widget --vendor-chunk=false --output-hashing=none && node concat.js src/assets/widgets/coin-price-marquee-widget/"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^7.3.1",
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^7.3.2",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/elements": "^7.2.5",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^7.3.2",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.0",
    "concat-files": "^0.1.1",
    "core-js": "^2.5.4",
    "ejs": "^3.1.7",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "rxjs": "6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
